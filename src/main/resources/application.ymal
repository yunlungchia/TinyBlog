server:
  port: 8085


# OkHttp
ok:
  http:
    connect-timeout: 30
    keep-alive-duration: 300
    max-idle-connections: 200
    read-timeout: 30
    write-timeout: 30


# Actuator
management:
  security:
    enabled: false #关掉安全认证
  port: 80
  context-path: /monitor #actuator的访问路径
endpoints:
  shutdown:
    enabled: true
  beans:
    id: instances